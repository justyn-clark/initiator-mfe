  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^9.0.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.1.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglify-js": "^2.7.4",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "watchify": "^3.7.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "eventemitter2": "^4.1.2",
    "jquery": "^3.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "scripts": {
    "start": "npm-run-all -p sync watch:css bundle",
    "test": "eslint src/js",
    "sync": "browser-sync start --server --files \"*.html, app/bundle.js, app/app.css\"",
    "build:css": "node-sass --include-path scss src/**/*.scss app/app.css --source-map-embed --source-map-contents --output-style compressed",
    "build": "npm run bundle && npm run build:css",
    "bundle": "watchify src/app.js -d -t [ babelify  --presets [ env ] ] --outfile app/bundle.js -v",
    "postcss": "postcss --use autoprefixer -o app/app.css app/app.css",
    "watch:css": "nodemon -e scss -x \"npm-run-all build:css postcss\"",
    "babel-node": "babel-node src/js/app.js",
    "ug": "uglifyjs app/bundle.js --compress --mangle --output app/bundle.min.js --source-map app/bundle.min.js.map",
    "webpack:build": "webpack -p --env production",
    "webpack:start": "webpack-dev-server --open"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "main": "./dist/index.html"
}
